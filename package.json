{
	"name": "app",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"dev": "cross-env yarn env-cmd -f ./deployments/.env.local next dev",
		"build:dev": "cross-env yarn env-cmd -f ./deployments/.env.local next build",
		"build:stag": "cross-env yarn env-cmd -f ./deployments/.env.staging next build",
		"build:prod": "cross-env yarn env-cmd -f ./deployments/.env.production next build",
		"deploy:stag": "yarn build:stag && wrangler pages deploy ./out --project-name app --branch staging",
		"deploy:prod": "yarn build:prod && wrangler pages deploy ./out --project-name app --branch main",
		"start": "next start",
		"lint": "next lint",
		"typecheck": "tsc --noEmit -p tsconfig.json",
		"prepare": "husky install",
		"gen:ts": "node scripts/gen-model-typescript.js",
		"prettier:check": "prettier --write ./src",
		"test": "playwright test --project=chromium",
		"test:headless": "HEADLESS_MODE=true playwright test --project=chromium",
		"start:server": "serve ./node_modules/@metamask/test-dapp/dist -p 3000",
		"configbot": "node configure.js"
	},
	"dependencies": {
